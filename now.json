{
  "version": 2,
  "alias": ["tft.willcodefor.beer"],
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    {"src": "/index.js", "headers": {"cache-control": "max-age=31536000,immutable"}, "dest": "/index.js"},
    {"src": "/(.*)", "headers": {"cache-control": "max-age=0,must-revalidate"}, "dest": "/index.html"}
  ],
  "regions": ["arn"]
}
