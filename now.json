{
  "version": 2,
  "alias": ["tft.willcodefor.beer"],
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    {
      "src": "/*.js",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "$1"
    },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "max-age=0,must-revalidate" },
      "dest": "/index.html"
    }
  ],
  "regions": ["arn"]
}
